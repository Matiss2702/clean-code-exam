{
  "importMap": "./import_map.json",
  "compilerOptions": {
    "strict": true,
    "lib": ["deno.ns", "deno.unstable", "esnext"],
    "target": "ES2020"
  },
  "tasks": {
    "start": "deno run --allow-net --allow-env --unstable-sloppy-imports --import-map=import_map.json main.ts",
    "test": "deno test --allow-net --allow-env --unstable-sloppy-imports --import-map=import_map.json"
  }
}
